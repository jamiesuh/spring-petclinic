pipeline {
    agent any
  
  stages {
    stage('Git fatch') {
      steps {
        echo 'git fatch'
         git url: 'https://github.com/jamiesuh/spring-petclinic.git/',
             branch: main, credentialID: 'gitCredential'
      }
    }
    stage('Maven Build') {
      steps {
        echo 'maven build'
      }
    }  
    stage('Docker Image') {
      steps {
        echo 'docker image'
      }
    }  
    stage('Image Push') {
      steps {
        echo 'image push'
      }
    }
    stage('Codedeploy') {
      steps {
        echo 'codedeploy'
      }
    }
  }
}
